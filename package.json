{
  "name": "test-three-fiber",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.12.1"
  },
  "dependencies": {
    "@react-three/drei": "^9.56.25",
    "@react-three/fiber": "^8.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "sass": "^1.58.3",
    "three": "^0.149.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint.typescript": "yarn tsc --noEmit",
    "lint.eslint": "eslint './src/**/*.{js,ts,tsx}'"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "airbnb-typescript",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "import",
      "react"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "parserOptions": {
      "project": [
        "tsconfig.json"
      ]
    },
    "overrides": [
      {
        "files": [
          "src/**/*.tsx",
          "src/**/*.ts"
        ]
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/sass": "^1.43.1",
    "@types/three": "^0.149.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true,
    "printWidth": 90,
    "tabWidth": 2
  }
}
